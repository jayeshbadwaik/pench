\setbeamercolor{footline}{fg=slblue, bg=}

\setbeamercolor{inverse}{fg=slwhite,bg=slblue}

\setbeamercolor{normal text}{fg=black,bg=white}
\setbeamercolor{date in head/foot}{fg=structure}

\setbeamercolor{alerted text}{fg=slred}
\setbeamercolor{example text}{fg=sllightblue}
\setbeamercolor{emphasized text}{fg=slblue}

\setbeamercolor{part name}{fg=white}
