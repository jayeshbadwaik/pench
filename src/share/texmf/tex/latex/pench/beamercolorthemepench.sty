\setbeamercolor{note page}{bg=white, fg=black}
\setbeamercolor{note title}{bg=sllightblue!50!white, fg=black}
\setbeamercolor{note date}{fg=slblue}

\setbeamercolor{structure}{fg=slblue,bg=}
\setbeamercolor{palette primary}{use=structure,fg=slblue,bg=}

\setbeamercolor{title}{fg=white,bg=slblue}
\setbeamercolor{subtitle}{fg=sllightblue}

\setbeamercolor{header}{fg=slblue,bg=}
\setbeamercolor{navigation symbols}{fg=slgray,bg=}

\setbeamercolor{frametitle}{fg=slblue,bg=}
