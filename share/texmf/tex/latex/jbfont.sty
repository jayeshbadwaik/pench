\ProvidesPackage{jbfont}

\RequirePackage{fontspec}

\RequirePackage{microtype}

\RequirePackage{unicode-math}

\setmainfont{Linux Libertine O}
\setmathfont
[
%Scale=MatchLowercase,
%math-style=upright
]
{Latin Modern Math}

\setmathfont
[
%Scale=MatchLowercase,
range={bb}
]{TeX Gyre Pagella Math}

\setmathfont
[
%Scale=MatchLowercase,
range={scr,bfscr}
]{Xits Math}

\setmathfont
[
%Scale=MatchLowercase,
range=\setminus
]{XITS Math}



